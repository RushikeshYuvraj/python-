def is_leap(year):
	""" Function to find the given
	year is leap or not."""
    if year%100 == 0 and year%400 == 0:
       print(f"{year} is a leap year.")
    elif year%4 == 0:
         print(f"{year} is a leap year.")
    else:
        print(f'{year} is not a leap year.')


y = int(input())
is_leap(y)


